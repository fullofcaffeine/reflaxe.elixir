	function error(message:String, pos:Null<haxe.macro.Position>):Void;

	function getExpressionBuilder():haxe.macro.TypedExpr -> reflaxe.elixir.ast.ElixirAST;

	function getTypeBuilder():haxe.macro.Type -> reflaxe.elixir.ast.ElixirAST;

	function getPatternBuilder(clauseContext:reflaxe.elixir.ast.context.ClauseContext):haxe.macro.TypedExpr -> reflaxe.elixir.ast.ElixirAST;

	function registerBuilder(builderType:String, builder:reflaxe.elixir.ast.builders.IBuilder):Void;

	function isFeatureEnabled(flag:String):Bool;

	function setFeatureFlag(flag:String, enabled:Bool):Void;
