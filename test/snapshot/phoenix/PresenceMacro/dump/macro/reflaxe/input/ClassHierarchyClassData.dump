class reflaxe.input.ClassHierarchyClassData {

	public function new(cls:haxe.macro.ClassType) {
		this.cls = cls;
		this.children = [];
	}

	public var cls:haxe.macro.ClassType;

	public var children:Array<haxe.macro.ClassType>;

	public function addChild(c:haxe.macro.ClassType) {
		if (! this.children.contains(c)) {
			this.children.push(c);
		};
	}
}