												if (snake == binder) {
													var newBodyBinder = reflaxe.elixir.ast.transformers.ClauseUndefinedRefRewriteTransforms.replaceVar(cl.body, targetName, binder);
													out.push({pattern : cl.pattern, guard : cl.guard, body : newBodyBinder});
													continue;
