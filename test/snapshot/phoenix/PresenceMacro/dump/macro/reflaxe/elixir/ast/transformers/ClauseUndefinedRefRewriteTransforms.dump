													false;
												} else {
													var c = u.charAt(0);
													c.toLowerCase() == c;
												};
											}) {
												undef.push(u);
											};
										};
										if (undef.length == 1) {
											var targetName = undef[0];
											var snake = reflaxe.elixir.ast.NameUtils.toSnakeCase(targetName);
											if (snake != null && snake != targetName) {
