			case EVar(n):
				allDefined.set(n, true);
				if (n.length > 1 && n.charAt(0) == "_") allUnders.set(n, true);			
			default:
		};	
	case EMatch(PVar(n2), _):
		allDefined.set(n2, true);
		if (n2.length > 1 && n2.charAt(0) == "_") allUnders.set(n2, true);	
	default:
}) {
