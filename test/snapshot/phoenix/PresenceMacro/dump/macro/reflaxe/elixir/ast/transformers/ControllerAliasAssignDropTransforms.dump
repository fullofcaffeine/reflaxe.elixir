	static function filter(stmts:Array<reflaxe.elixir.ast.ElixirAST>) {
		if (stmts == null) {
			return stmts;
		};
		var out = [];
		{
			var ` = 0;
			while (` < stmts.length) {
				var s = stmts[`];
				++ `;
				if (reflaxe.elixir.ast.transformers.ControllerAliasAssignDropTransforms.isAssignTo(s, "json") || reflaxe.elixir.ast.transformers.ControllerAliasAssignDropTransforms.isAssignTo(s, "data") || reflaxe.elixir.ast.transformers.ControllerAliasAssignDropTransforms.isAssignTo(s, "conn")) {
					continue;
