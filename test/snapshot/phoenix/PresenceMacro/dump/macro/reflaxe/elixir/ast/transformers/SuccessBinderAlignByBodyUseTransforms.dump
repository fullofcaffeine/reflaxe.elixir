	static inline function isLower(s:String) {
		if (s == null || s.length == 0) {
			return false;
		};
		var c = s.charAt(0);
		return c.toLowerCase() == c;
	}

	static inline function cloneMap<T>(m:Map<String, cloneMap.T>) {
		var n = {
			{};
			new haxe.ds.StringMap();
		};
		if (m == null) {
			return n;
		};
		for (k in m.keys()) {
			{
				var value = m.get(k);
				n.set(k, value);
			};
		};
		return n;
	}
