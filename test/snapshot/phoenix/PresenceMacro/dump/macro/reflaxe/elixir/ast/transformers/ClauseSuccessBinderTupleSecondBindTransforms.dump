	static function allow(name:String) {
		if (name == null || name.length == 0) {
			return false;
		};
		if (name == "socket" || name == "params" || name == "_params" || name == "event") {
			return false;
		};
		var c = name.charAt(0);
		return c.toLowerCase() == c && c != "_";
	}

