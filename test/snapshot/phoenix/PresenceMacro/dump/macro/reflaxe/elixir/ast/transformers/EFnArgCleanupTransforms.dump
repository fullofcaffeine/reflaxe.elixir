		visit(tgt2);
		for (a  in  args2) visit(a);	
	case ETuple(els):
		for (el  in  els) visit(el);	
	case EMap(pairs):
		for (p  in  pairs) {
			visit(p.key);
			visit(p.value);
		};	
	case EFn(clauses):
		for (cl  in  clauses) visit(cl.body);	
