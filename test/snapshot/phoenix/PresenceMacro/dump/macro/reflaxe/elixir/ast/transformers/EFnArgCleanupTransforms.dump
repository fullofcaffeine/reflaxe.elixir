		for (s  in  sts) visit(s);	
	case EIf(c, t, e):
		visit(c);
		visit(t);
		if (e != null) visit(e);	
	case EBinary(_, l, r):
		visit(l);
		visit(r);	
	case EUnary(_, e1):
		visit(e1);	
	case ECall(tgt, _, args):
		if (tgt != null) visit(tgt);
		for (a  in  args) visit(a);	
