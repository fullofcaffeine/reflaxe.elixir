	public static function pass(ast:reflaxe.elixir.ast.ElixirAST) {
		return reflaxe.elixir.ast.ElixirASTTransformer.transformNode(ast, function(n:reflaxe.elixir.ast.ElixirAST) {
			return @:ast(switch (n.def) {
	case EDef(name, args, guards, body):
		var p = detectQueryParam(args);
		if (p == null) n else makeASTWithMeta(EDef(name, args, guards, synthesize(body, p)), n.metadata, n.pos);	
	case EDefp(name2, args2, guards2, body2):
		var p2 = detectQueryParam(args2);
		if (p2 == null) n else makeASTWithMeta(EDefp(name2, args2, guards2, synthesize(body2, p2)), n.metadata, n.pos);	
	default:
		n;	
}) {
				var ` = n.def;
				switch (enumIndex `) {
					case 2: {
						var ` = `[0];
						var ` = `[1];
						var ` = `[2];
						var ` = `[3];
						{
							var name = `;
							var args = `;
							var guards = `;
							var body = `;
							{
								var p = reflaxe.elixir.ast.transformers.FunctionQueryBinderSynthesisTransforms.detectQueryParam(args);
								if (p == null) {
									n;
								} else {
									{
										var def = reflaxe.elixir.ast.ElixirASTDef.EDef(name, args, guards, reflaxe.elixir.ast.transformers.FunctionQueryBinderSynthesisTransforms.synthesize(body, p));
										var meta = n.metadata;
										var pos = n.pos;
										{def : def, metadata : meta, pos : pos};
									};
								};
							};
						};
					};
					case 3: {
						var ` = `[0];
						var ` = `[1];
						var ` = `[2];
						var ` = `[3];
						{
							var name2 = `;
							var args2 = `;
							var guards2 = `;
							var body2 = `;
							{
								var p2 = reflaxe.elixir.ast.transformers.FunctionQueryBinderSynthesisTransforms.detectQueryParam(args2);
								if (p2 == null) {
									n;
								} else {
									{
										var def = reflaxe.elixir.ast.ElixirASTDef.EDefp(name2, args2, guards2, reflaxe.elixir.ast.transformers.FunctionQueryBinderSynthesisTransforms.synthesize(body2, p2));
										var meta = n.metadata;
										var pos = n.pos;
										{def : def, metadata : meta, pos : pos};
									};
								};
							};
						};
					};
					default: {
						n;
					}
				};
			};
