		pat(h, d);
		pat(t, d);	
	case PMap(kvs):
		for (kv  in  kvs) pat(kv.value, d);	
	case PStruct(_, fs):
		for (f  in  fs) pat(f.value, d);	
	case PPin(inner):
		pat(inner, d);	
	default:
}) switch (enumIndex p) {
