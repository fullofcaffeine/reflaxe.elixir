	}

	static function detectQueryParam(args:Array<reflaxe.elixir.ast.EPattern>) {
		if (args == null) {
			return null;
		};
		var found = null;
		var count = 0;
		{
			var ` = 0;
			while (` < args.length) {
				var a = args[`];
				++ `;
				@:ast(switch (a) {
	case PVar(n) if (StringTools.endsWith(n, "_query")):
		found = n;
		count++;	
	default:
}) if (enumIndex a == 0) {
					var ` = a[0];
					{
						var n = `;
						if (StringTools.endsWith(n, "_query")) {
							found = n;
							count ++;
						} else {};
					};
				} else {};
			};
		};
		return if (count == 1) {
			found;
		} else {
			null;
		};
	}

	static function synthesize(body:reflaxe.elixir.ast.ElixirAST, paramName:String) {
		var declared = {
			{};
			new haxe.ds.StringMap();
		};
		var usesQuery = [false];
		reflaxe.elixir.ast.ASTUtils.walk(body, function(x:reflaxe.elixir.ast.ElixirAST) {
			@:ast(switch (x.def) {
	case EVar(nm) if (nm == "query"):
		usesQuery = true;	
	case EMatch(p, _):
		pat(p, declared);	
	case EBinary(Match, l, _):
		lhs(l, declared);	
	default:
}) {
				var ` = x.def;
				switch (enumIndex `) {
					case 8: {
						var ` = `[0];
