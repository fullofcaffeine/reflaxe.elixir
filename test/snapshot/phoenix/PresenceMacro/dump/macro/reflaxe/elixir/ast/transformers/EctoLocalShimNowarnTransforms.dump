				var pos = null;
				{def : def, metadata : {}, pos : pos};
			}});
		};
		var value = {
			var def = reflaxe.elixir.ast.ElixirASTDef.ETuple([{
				var def = reflaxe.elixir.ast.ElixirASTDef.EAtom(@:implicitCast {
					var this;
					this = reflaxe.elixir.ast.NameUtils.toSnakeCase("nowarn_unused_function");
					cast this;
				});
				var pos = null;
				{def : def, metadata : {}, pos : pos};
			}, {
