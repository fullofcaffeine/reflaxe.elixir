	static function buildNowarnAttr(body:Array<reflaxe.elixir.ast.ElixirAST>) {
		var arities = {
			{};
			new haxe.ds.StringMap();
		};
		{
			var ` = 0;
			while (` < body.length) {
				var b = body[`];
				++ `;
				@:ast(switch (b.def) {
	case EDefp(fname, args, _, _):
		if (fname == "from" || fname == "where") {
			arities.set(fname, args != null ? args.length : 0);
		};	
	default:
}) {
					var ` = b.def;
					if (enumIndex ` == 3) {
						var ` = `[0];
						var ` = `[1];
						var ` = `[2];
						var ` = `[3];
						{
