	}

	static function appendVar(b:reflaxe.elixir.ast.ElixirAST, name:String) {
		return @:ast(switch (b.def) {
	case EBlock(stmts):
		makeASTWithMeta(EBlock(stmts.concat([makeAST(EVar(name))])), b.metadata, b.pos);	
	default:
		b;	
}) {
			var ` = b.def;
			if (enumIndex ` == 53) {
				var ` = `[0];
				{
					var stmts = `;
					{
						{
							var def = reflaxe.elixir.ast.ElixirASTDef.EBlock(stmts.concat([{
								var pos = null;
								{def : reflaxe.elixir.ast.ElixirASTDef.EVar(name), metadata : {}, pos : pos};
							}]));
							var meta = b.metadata;
							var pos = b.pos;
							{def : def, metadata : meta, pos : pos};
						};
					};
				};
			} else {
