						if (name == "changeset") {
							var needsCs = reflaxe.elixir.ast.transformers.EctoChangesetReturnFixTransforms.detectsCsAssign(body) && ! reflaxe.elixir.ast.transformers.EctoChangesetReturnFixTransforms.endsWithVar(body, "cs");
							if (needsCs) {
								var nb = reflaxe.elixir.ast.transformers.EctoChangesetReturnFixTransforms.appendVar(body, "cs");
								{def : reflaxe.elixir.ast.ElixirASTDef.EDef(name, args, guards, nb), metadata : n.metadata, pos : n.pos};
							} else {
								n;
							};
