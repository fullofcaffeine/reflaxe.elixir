	static function countVars(body:reflaxe.elixir.ast.ElixirAST) {
		var m = new haxe.ds.StringMap();
		reflaxe.elixir.ast.ASTUtils.walk(body, function(x:reflaxe.elixir.ast.ElixirAST) {
			@:ast(switch (x.def) {
	case EVar(n):
