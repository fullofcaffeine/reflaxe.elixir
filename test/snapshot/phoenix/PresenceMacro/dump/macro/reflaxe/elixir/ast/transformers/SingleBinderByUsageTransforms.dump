	static inline function isLower(s:String) {
		var c = s.charAt(0);
		return c.toLowerCase() == c;
	}

	static inline function allowCandidate(name:String) {
		if (name == null || name.length == 0) {
			return false;
		};
		if (name == "socket" || name == "live_socket" || name == "liveSocket") {
			return false;
		};
		return {
			var c = name.charAt(0);
			c.toLowerCase() == c;
		};
	}

	static function toSnake(s:String) {
		if (s == null || s.length == 0) {
			return s;
		};
		var buf = new StringBuf();
		{
			var ` = 0;
			var ` = s.length;
			while (` < `) {
				var i = ` ++;
				var c = s.substr(i, 1);
				var lower = c.toLowerCase();
				var upper = c.toUpperCase();
				if (c == upper && c != lower) {
					if (i != 0) {
						buf.add("_");
					};
					buf.add(lower);
				} else {
					buf.add(c);
				};
			};
		};
		return buf.toString();
	}

	static function isDefinedName(name:String, defined:Map<String, Bool>) {
		if (defined.exists(name)) {
			return true;
		};
		var snake = reflaxe.elixir.ast.transformers.SingleBinderByUsageTransforms.toSnake(name);
		if (snake != name && defined.exists(snake)) {
			return true;
		};
		return false;
	}
