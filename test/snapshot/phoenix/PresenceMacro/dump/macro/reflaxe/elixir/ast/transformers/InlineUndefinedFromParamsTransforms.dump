	case EMatch(p, _):
		collectPattern(p, out);	
	case EBinary(Match, l, _):
		collectLhs(l, out);	
	case ECase(_, cs):
		for (c  in  cs) collectPattern(c.pattern, out);	
	default:
}) {
				var ` = n.def;
				switch (enumIndex `) {
					case 6: {
