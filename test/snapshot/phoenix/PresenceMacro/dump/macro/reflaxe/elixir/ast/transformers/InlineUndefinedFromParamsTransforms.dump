	static function findParamsVar(args:Array<reflaxe.elixir.ast.EPattern>) {
		if (args == null || args.length == 0) {
			return null;
		};
		{
			var ` = 0;
			while (` < args.length) {
				var a = args[`];
				++ `;
				@:ast(switch (a) {
	case PVar(n):
		if (n == "params" || n == "_params") return n;	
	default:
}) if (enumIndex a == 0) {
					var ` = a[0];
					{
						var n = `;
						{
							if (n == "params" || n == "_params") {
								return n;
							};
						};
					};
				} else {};
			};
		};
		return null;
	}

	static inline function toSnake(s:String) return reflaxe.elixir.ast.NameUtils.toSnakeCase(s)

	static inline function allow(name:String) {
		if (name == null || name.length == 0) {
			return false;
		};
		if (name == "socket" || name == "params" || name == "_params" || name == "event") {
			return false;
		};
		var c = name.charAt(0);
		return c.toLowerCase() == c && c != "_";
	}

	static function isCamel(name:String) {
		var result = false;
		if (name != null && name.length > 0) {
			var c0 = name.charAt(0);
			if (c0.toLowerCase() == c0) {
				var i = 1;
				while (i < name.length) {
					var ch = name.charAt(i);
					if (ch.toUpperCase() == ch && ch.toLowerCase() != ch) {
						result = true;
						break;
					};
					i ++;
				};
			};
		};
		return result;
	}

	static function needsIntConversion(varName:String) {
		return varName == "id" || StringTools.endsWith(varName, "_id");
	}

	static function buildExtract(varName:String, paramsVar:String) {
		var key = reflaxe.elixir.ast.NameUtils.toSnakeCase(varName);
		var get = {
			var pos = null;
			{def : reflaxe.elixir.ast.ElixirASTDef.ERemoteCall({
				var pos = null;
				{def : reflaxe.elixir.ast.ElixirASTDef.EVar("Map"), metadata : {}, pos : pos};
			}, "get", [{
				var pos = null;
				{def : reflaxe.elixir.ast.ElixirASTDef.EVar(paramsVar), metadata : {}, pos : pos};
			}, {
				var pos = null;
				{def : reflaxe.elixir.ast.ElixirASTDef.EString(key), metadata : {}, pos : pos};
			}]), metadata : {}, pos : pos};
		};
		if (! reflaxe.elixir.ast.transformers.InlineUndefinedFromParamsTransforms.needsIntConversion(varName)) {
			return get;
		};
		var isBin = {
			var pos = null;
			{def : reflaxe.elixir.ast.ElixirASTDef.ERemoteCall({
				var pos = null;
				{def : reflaxe.elixir.ast.ElixirASTDef.EVar("Kernel"), metadata : {}, pos : pos};
			}, "is_binary", [get]), metadata : {}, pos : pos};
		};
		var toInt = {
			var pos = null;
			{def : reflaxe.elixir.ast.ElixirASTDef.ERemoteCall({
				var pos = null;
				{def : reflaxe.elixir.ast.ElixirASTDef.EVar("String"), metadata : {}, pos : pos};
			}, "to_integer", [get]), metadata : {}, pos : pos};
		};
		return {
			var pos = null;
			{def : reflaxe.elixir.ast.ElixirASTDef.EIf(isBin, toInt, get), metadata : {}, pos : pos};
		};
	}

	static function inlineUndeclared(body:reflaxe.elixir.ast.ElixirAST, declared:Map<String, Bool>, paramsVar:String) {
		return reflaxe.elixir.ast.ElixirASTTransformer.transformNode(body, function(n:reflaxe.elixir.ast.ElixirAST) {
			return @:ast(switch (n.def) {
	case EVar(v) if ((allow(v) || isCamel(v)) && !declared.exists(v)):
		Sys.println("[InlineFromParams] Inlining " + v + " from " + paramsVar);
		buildExtract(v, paramsVar);	
	default:
		n;	
}) {
				var ` = n.def;
				if (enumIndex ` == 38) {
					var ` = `[0];
					{
						var v = `;
						if ((if (v == null || v.length == 0) {
							false;
						} else {
							if (v == "socket" || v == "params" || v == "_params" || v == "event") {
								false;
							} else {
								var c = v.charAt(0);
								c.toLowerCase() == c && c != "_";
							};
						} || reflaxe.elixir.ast.transformers.InlineUndefinedFromParamsTransforms.isCamel(v)) && ! declared.exists(v)) {
							Sys.println("[InlineFromParams] Inlining " + v + " from " + paramsVar);
							reflaxe.elixir.ast.transformers.InlineUndefinedFromParamsTransforms.buildExtract(v, paramsVar);
						} else {
							n;
						};
					};
				} else {
					n;
				};
			};
		});
	}

	static function collectDecls(ast:reflaxe.elixir.ast.ElixirAST, out:Map<String, Bool>) {
		reflaxe.elixir.ast.ASTUtils.walk(ast, function(n:reflaxe.elixir.ast.ElixirAST) {
			if (n == null || n.def == null) {
