	static inline function isReserved(name:String) {
		return name == "socket" || name == "live_socket" || name == "params";
	}

	static inline function allow(name:String) {
		if (name == null || name.length == 0) {
			return false;
		};
		if (name == "socket" || name == "live_socket" || name == "params") {
			return false;
		};
		var c = name.charAt(0);
		return c.toLowerCase() == c;
	}

