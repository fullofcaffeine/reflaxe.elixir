											} && ! declared.exists(u) && u != binder) {
												cands.push(u);
											};
										};
										var preferred = ["todo", "id", "message", "params", "reason"];
										var chosen = null;
										if (cands.length > 1) {
											{
												var ` = 0;
												while (` < preferred.length) {
