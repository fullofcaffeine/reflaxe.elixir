		};
		var out = new StringBuf();
		{
			var ` = 0;
			var ` = s.length;
			while (` < `) {
				var i = ` ++;
				var ch = s.charAt(i);
				var isUpper = (ch.toUpperCase() == ch && ch.toLowerCase() != ch);
				if (isUpper && i > 0) {
					out.add("_");
				};
				out.add(ch.toLowerCase());
			};
		};
		return out.toString();
	}

