	static function detectQueryParam(args:Array<reflaxe.elixir.ast.EPattern>) {
		if (args == null) {
			return null;
		};
		var one = null;
		var cnt = 0;
		{
			var ` = 0;
			while (` < args.length) {
				var a = args[`];
				++ `;
				@:ast(switch (a) {
	case PVar(n) if (StringTools.endsWith(n, "_query")):
		one = n;
		cnt++;	
	default:
}) if (enumIndex a == 0) {
					var ` = a[0];
					{
						var n = `;
						if (StringTools.endsWith(n, "_query")) {
							one = n;
							cnt ++;
						} else {};
					};
				} else {};
			};
		};
		return if (cnt == 1) {
			one;
		} else {
			null;
		};
	}

	static function isDowncaseAssignOf(param:String, stmt:reflaxe.elixir.ast.ElixirAST) {
