												if (` == "filter") {
													{
														var tgt = `;
														var args2 = `;
														if (args2 != null && args2.length >= 1) {
															Sys.println("[DowncaseParamThenFilter] rewriting Enum.filter(call) predicate query→" + param);
															var last = args2[args2.length - 1];
															var np2 = reflaxe.elixir.ast.transformers.DowncaseParamThenFilterPredicateNormalizeTransforms.rewriteQueryToParam(last, param);
															var prefix = args2.slice(0, args2.length - 1);
															out.push(s);
															out.push({
																var def = reflaxe.elixir.ast.ElixirASTDef.ECall(tgt, "filter", prefix.concat([np2]));
																var meta = next.metadata;
																var pos = next.pos;
																{def : def, metadata : meta, pos : pos};
															});
															i += 2;
															continue;
														} else {};
													};
												} else {};
											};
											case 24: {
												var ` = `[0];
