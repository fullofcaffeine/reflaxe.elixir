													if (enumIndex ` == 38) {
														var ` = `[0];
														if (` == "filter") {
															{
																var m = `;
																var args = `;
																if (m == "Enum" && args != null && args.length == 2) {
																	Sys.println("[DowncaseParamThenFilter] rewriting Enum.filter(remote) predicate query→" + param);
																	var np = reflaxe.elixir.ast.transformers.DowncaseParamThenFilterPredicateNormalizeTransforms.rewriteQueryToParam(args[1], param);
																	out.push(s);
																	out.push({def : reflaxe.elixir.ast.ElixirASTDef.ERemoteCall({
