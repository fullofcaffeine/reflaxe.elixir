	static function rewriteQueryToParam(pred:reflaxe.elixir.ast.ElixirAST, param:String) {
		return reflaxe.elixir.ast.ElixirASTTransformer.transformNode(pred, function(x:reflaxe.elixir.ast.ElixirAST) {
			return @:ast(switch (x.def) {
	case EVar(nm) if (nm == "query"):
		Sys.println("[DowncaseParamThenFilter] query -> " + param);
		makeASTWithMeta(EVar(param), x.metadata, x.pos);	
	default:
		x;	
}) {
				var ` = x.def;
				if (enumIndex ` == 38) {
					var ` = `[0];
					{
						var nm = `;
						if (nm == "query") {
