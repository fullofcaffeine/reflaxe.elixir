			while (` < params.length) {
				var p = params[`];
				++ `;
				reflaxe.elixir.ast.transformers.DefParamUnderscoreRefFixTransforms.collect(p, m);
			};
		};
		return m;
	}

	static function collect(p:reflaxe.elixir.ast.EPattern, m:Map<String, Bool>) {
