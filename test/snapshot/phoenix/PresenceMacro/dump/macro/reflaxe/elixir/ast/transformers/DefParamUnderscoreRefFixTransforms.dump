	case PList(es):
		for (e  in  es) collect(e, m);	
	case PCons(h, t):
		collect(h, m);
		collect(t, m);	
	case PMap(kvs):
		for (kv  in  kvs) collect(kv.value, m);	
	case PStruct(_, fs):
		for (f  in  fs) collect(f.value, m);	
	case PPin(inner):
		collect(inner, m);	
	default:
}) switch (enumIndex p) {
