	static function rewriteBody(body:reflaxe.elixir.ast.ElixirAST, params:Map<String, Bool>) {
		return reflaxe.elixir.ast.ElixirASTTransformer.transformNode(body, function(n:reflaxe.elixir.ast.ElixirAST) {
			return @:ast(switch (n.def) {
	case EVar(v) if (v != null && v.length > 1 && v.charAt(0) == "_"):
		var base = v.substr(1);
		if (params.exists(base) && !params.exists(v)) makeASTWithMeta(EVar(base), n.metadata, n.pos) else n;	
	default:
		n;	
}) {
				var ` = n.def;
				if (enumIndex ` == 38) {
					var ` = `[0];
					{
						var v = `;
						if (v != null && v.length > 1 && v.charAt(0) == "_") {
							var base = v.substr(1, null);
							if (params.exists(base) && ! params.exists(v)) {
								{def : reflaxe.elixir.ast.ElixirASTDef.EVar(base), metadata : n.metadata, pos : n.pos};
							} else {
